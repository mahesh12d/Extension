{
  "manifest_version": 3,
  "name": "AutoFill Pro",
  "version": "1.0",
  "description": "Automatically fill forms with stored profiles",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
